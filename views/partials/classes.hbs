<div class="jumbotron">
    <h2>Featured Classes</h2>
  <div style="padding-left: 15%;">
    <div class="row">
      {{#each classes}}
      <div class="col-md-5 col-md-offset-2 pr-5 animatedParent box">
        <div class="animated growIn">
          <h4>{{title}}</h4>
          <p>{{description}}</p>
          <br />
          <a href="/classes/{{_id}}/details" class="cool-link">View Class</a>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
