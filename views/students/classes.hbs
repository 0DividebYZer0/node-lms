<div class="container-fluid h-100">
     <div class="row h-100">
       <main class="col bg-faded py-3">
         <span class="text-center">
           <h1>{{student.first_name}}'s Dashboard</h1>
         </span>
         <div class="container">
           <table class="table">
             <thead class="" style="background-color: #b8bad6;">
               <tr>
                 <th scope="col">ClassID</th>
                 <th scope="col">Class Name</th>
                 <th></th>
               </tr>
             </thead>
             <tbody>
               {{#each student.classes}}
                 <tr>
                   <td>{{class_id}}</td>
                   <td>{{class_title}}</td>
                   <td><a href="/classes/{{class_id}}/lessons">View Lessons</a></td>
                 </tr>
               {{/each}}
             </tbody>
           </table>
         </div>
       </main>
     </div>
   </div>
